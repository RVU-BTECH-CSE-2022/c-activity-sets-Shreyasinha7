#include <stdio.h>
int input_array_size();
void input_array(int n, int a[n]);
int sum_n_array(int n, int a[n]);
void output(int n, int a[n],int sum);

int main()
  int n,sum;
  n = input_array_size();
  int a[n];
  input_array(n,a);
  sum =sum_n_array(n,a);
  output(n,a,sum);
}

int input_array_size(){
  int x;
  printf("Enter a size\n");
  scanf("%d", &x);
  return(x);
}